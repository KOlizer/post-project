<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
        <!--클래스 alias가 들어갈 공간-->
        <typeAlias type="com.example.post_project.post.Post" alias="post"></typeAlias>
        <typeAlias type="com.example.post_project.user.User" alias="user"></typeAlias>
        <typeAlias type="com.example.post_project.controller.IndexController" alias="IndexController"></typeAlias>
        <!--indexController예시-->
    </typeAliases>
</configuration>
